<%= render :partial => 'shared/filter' -%>

<div id="users">
  <%=render :partial => 'users'%>
</div>

<script>
  $( function() {
    $('a.users').addClass( 'current' );

    $('a.nav_link').click( function() {
      $("a.nav_link").removeClass( "current" );   
      $(this).addClass( 'current' );    
      return true;
    });
    
    $('.pagination a').live( 'click', function() {
      $("#content").html( '<%=render "wait"%>' ); 
      $.getScript( this.href );
      return false;
    });  
  });
</script>

